{
  "manifest_version": 3,
  "name": "Batch URL Fetcher",
  "version": "1.0",
  "description": "CSVインポートからURLリストを読み込み、レスポンスをダウンロードする拡張",
  "permissions": ["downloads", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}